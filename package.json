{
    "name": "whistleapp",
    "main": "main.js",
    "version": "4.0.0",
    "scripts": {
        "dev": "./node_modules/electron/dist/Electron.app/Contents/MacOS/Electron ./",
        "buildWin": "node ./node_modules/electron-packager/cli.js ./ whistleApp --platform=win32 --arch=x64 --electron-version=4.0.0 --out=dist/ --overwrite=true --icon=./whistle.ico",
        "buildMac": "export ELECTRON_MIRROR=https://npm.taobao.org/mirrors/electron/ && node ./node_modules/electron-packager/cli.js ./ whistleApp --platform=darwin --arch=x64 --electron-version=4.0.0 --out=dist/ --overwrite=true --icon=./whistle.icns"
    },
    "dependencies": {
        "rollup-plugin-alias": "^1.5.1",
        "whistle": "^1.13.11"
    },
    "devDependencies": {
        "electron": "^4.0.0",
        "electron-packager": "^13.0.1"
    }
}
