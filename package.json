{
    "name": "whistleapp",
    "main": "main.js",
    "version": "4.5.0",
    "scripts": {
        "dev": "npx electron ./",
        "buildWin": "npx electron-packager ./ whistleApp --platform=win32 --arch=x64 --electron-version=4.0.0 --out=dist/ --overwrite=true --icon=./whistle.ico",
        "buildMac": "export ELECTRON_MIRROR=https://npm.taobao.org/mirrors/electron/ && npx electron-packager ./ whistleApp --platform=darwin --arch=x64 --electron-version=4.0.0 --out=dist/ --overwrite=true --icon=./whistle.icns"
    },
    "dependencies": {
        "rollup-plugin-alias": "^2.0.0",
        "whistle": "^1.16.5"
    },
    "devDependencies": {
        "electron": "^6.0.7",
        "electron-packager": "^14.0.5"
    }
}
